CXXFLAGS=	$(CFLAGS) -DQWS -fno-exceptions -fno-rtti -Wall -W -Wno-deprecated -O2 -mcpu=iwmmxt -mtune=iwmmxt -DNO_DEBUG
INCPATH	=	-I$(SDK_INC_DIR) -I$(SDK_SDL_INC_DIR)
LINK	=	$(SDK_CXX)
LFLAGS	=	$(SDK_LFLAGS)
LIBS	=	$(SUBLIBS) -L$(SDK_EZX_LIB_DIR) -lSDLmain -lSDL_image -lSDL_ttf
STRIP	=	$(SDK_STRIP)
MOC		=	$(QTDIR)/bin/moc
UIC		=	$(QTDIR)/bin/uic

TAR		=	tar -cf
GZIP	=	gzip -9f

